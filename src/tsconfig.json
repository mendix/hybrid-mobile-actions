{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "outDir": "../dist/tsc",
        "target": "es2015",
        "lib": ["es2015", "dom"],
        "types": ["mendix-client", "big.js", "cordova", "cordova-plugin-app-version"],
        "moduleResolution": "node",
        "module": "es2015",
        "sourceMap": false,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noUnusedLocals": false,
        "skipLibCheck": true,
        "removeComments": false,
        "alwaysStrict": false
    }
}
